<%
  imagePath = "./images";
%>

<% const cardItem = [
  {category:"カテゴリ１", src:imagePath + "/work/work01.jpg", alt:"株式会社CodeUps", title:"株式会社CodeUps" },
  {category:"カテゴリ１", src:imagePath + "/work/work02.jpg", alt:"株式会社CodeUps2", title:"株式会社CodeUps2" },
  {category:"カテゴリ１", src:imagePath + "/work/work03.jpg", alt:"株式会社CodeUps3", title:"株式会社CodeUps3" },
  {category:"カテゴリ１", src:imagePath + "/work/work04.jpg", alt:"株式会社CodeUps4", title:"株式会社CodeUps4" },
] %>

  <div class="p-related-work-cards">
    <ul class="p-related-work-cards__items">
      <% for (var item of cardItem) {%>
        <li class="p-related-work-cards__item">
          <%- include('./_p-work-card',{src:item.src, alt:item.alt, title:item.title, excerpt:item.excerpt, category:item.category, datetime:item.datetime, time:item.time}) %>
        </li>
      <% } %>
    </ul>
  </div>