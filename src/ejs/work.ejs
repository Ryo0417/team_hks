<!DOCTYPE html>
<html lang="ja">
  <%- include('./common/_head') %>
  <body>
		<% var location = {page_id:'work'} %>

    <!-- ヘッダー -->
    <header class="l-header">
      <%- include('./common/_p-header') %>
    </header>

    <!-- 下層ファーストビュー -->
    <%- include('./project/_p-sub-fv', {page: location}) %>

    <section class="p-sub-work-cards-head l-sub-work-cards-head">
      <div class="p-sub-work-cards-head__inner">
        <!-- パンくずリスト -->
        <div class="p-sub-work-cards-head__breadcrumb">
          <%-include('./component/_c-breadcrumb',{
            categoryName:'制作実績',
            postName:'',
            src:'work.html'
            })
          %>
        </div>
				<!-- カテゴリーリスト -->
				<div class="p-sub-blog__category">
					<%- include('./component/_c-category-list') %>
				</div>

				<div class="p-sub-work-cards-head__list">
					<%- include('./project/_p-sub-work-cards', {page: location}) %>
				</div>
        <!-- ページネーション -->
        <div class="p-sub-blog__pagination">
          <%- include('./component/_c-pagination') %>
        </div>
      </div>
    </section>
		<!-- お問い合わせ -->
		<section class="l-common-contact">
			<%- include('./project/_p-common-contact') %>
		</section>

    <!-- フッター -->
    <%- include('./common/_p-footer') %>

    <!-- ページトップ -->
    <%- include('./component/_c-pagetop') %>
  </body>
</html>