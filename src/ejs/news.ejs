<!DOCTYPE html>
<html lang="ja">
  <%- include('./common/_head') %>
  <body>
    <% var location = {page_id:'news'} %>

    <!-- ヘッダー -->
    <header class="l-header">
      <%- include('./common/_p-header') %>
    </header>

    <!-- 下層ファーストビュー -->
    <%- include('./project/_p-sub-fv', {page: location}) %>
    
    <!-- お知らせセクション -->
    <section class="p-sub-news l-sub-news"> 
      <div class="l-inner">
        <!-- パンくずリスト -->
        <div class="p-sub-news__breadcrumb">
          <%-include('./component/_c-breadcrumb',{
            categoryName:'お知らせ一覧',
            postName:'',
            src:'news.html'
            })
          %>
        </div>
      </div>

      <div class="p-sub-news__inner">
        <!-- お知らせリスト -->
        <div class="p-sub-news__items">
          <%- include('./project/_p-news-list') %>
        </div>
        <!-- ページネーション -->
        <div class="p-sub-news__pagination">
          <%- include('./component/_c-pagination') %>
        </div>
      </div>
    </section>

    <!-- お問い合わせ -->
    <section class="l-common-contact">
      <%- include('./project/_p-common-contact') %>
    </section>

    <!-- フッター -->
    <%- include('./common/_p-footer') %>

    <!-- ページトップ -->
    <%- include('./component/_c-pagetop') %>
  </body>
</html>
