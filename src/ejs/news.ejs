<!DOCTYPE html>
<html lang="ja">
  <%- include('./common/_head') %>
  <body>
    <% var location = {page_id:'news'} %>

    
    <!-- ヘッダー -->
    <header class="l-header">
      <%- include('./common/_p-header') %>
    </header>
    
    <!-- 下層ファーストビュー -->
    <%- include('./project/_p-sub-fv', {page: location, name: 'news', alt: 'お知らせ', title: 'お知らせ'}) %>
    
    <!-- パンくずリスト -->
    <div class="l-breadcrumb">
      <div class="l-inner">
        <%- include('./component/_c-breadcrumb',{
            categoryName:'お知らせ一覧',
            postName:'',
            src:'news.html'
          })
        %>
      </div>
    </div>
    
    <!-- お知らせセクション -->
    <section class="l-sub-news">
      <%- include('./project/_p-sub-news') %>
    </section>

    <!-- お問い合わせ -->
    <section class="l-common-contact">
      <%- include('./project/_p-common-contact') %>
    </section>

    <!-- フッター -->
    <%- include('./common/_p-footer', {page: location}) %>

    <!-- ページトップ -->
    <%- include('./component/_c-pagetop') %>
  </body>
</html>
